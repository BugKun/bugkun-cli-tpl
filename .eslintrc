{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "settings": {
        "import/ignore": [
            "node_modules"
        ]
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "rules": {
        
    },
    "globals": {
       
    }
}
